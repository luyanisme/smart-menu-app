/* progress.wxss */
page {
  height: 100%;
  overflow: hidden;
}

.wrapper {
  height: 100%;
  overflow: hidden;
  /*display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;*/
}

.case-item{
  width: 100%;
  height: 80px;
  position: relative;
  display: flex;
}

.case-title-wrapper{
position: absolute;
bottom:40px;
left: 10px;
display: flex;
flex-direction: row;
align-items: flex-end;
}

.case-title{
  font-size: 18px;
  color: #000000;
}

.case-property{
 font-size: 12px;
color: #a4a4a4;
}

.bottom-line{
  position: absolute;
  bottom: 0px;
  left: 0px;
  background-color: #f4f4f4;
  width: 100%;
  height: 1px;
}

.reminder-case{
  width: 100%;
  height: 40px;
  background-color: #6AD118;
  position: absolute;
  bottom: 0px;
  left: 0px;
  display: flex;
  justify-content: center;
  align-items: center;
}
@import "../../template/progressBar/progressBar.wxss"
